import socket, sys

def main(args):
    if len(args) < 3:
        print "eneter url and port"
        exit(1)

    s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    s.connect((args[1], int(args[2])))
    s.send("GET / HTTP/1.0\r\n\r\n")
   
    #directly from sockets page
    while True:
        buf = s.recv(1000)
        if not buf:
            break
        sys.stdout.write(buf)

    s.close()
    print

if __name__ == '__main__':
    main(sys.argv)
=======
import socket
import sys

def main(args):
    s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    s.connect((args[1], int(args[2])))
    request = "GET / HTTP/1.0\r\n\r\n"
    s.send(request)

    while True:
        buf = s.recv(1000)
        if not buf:
            break
        sys.stdout.write(buf)

    s.close()
    print

if __name__ == '__main__':
    main(sys.argv)
